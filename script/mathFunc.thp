DEPEND #mt
DEPEND >returnRegister

CONST l_m_half 20
CONST l_m_double 30

MACRO f_m_half {
        GETINDEX $returnRegister
        ADD 2 $returnRegister $returnRegister
        GOTO #l_m_half
}

MACRO f_m_double {
        GETINDEX $returnRegister
        ADD 2 $returnRegister $returnRegister
        GOTO #l_m_double
}



LABEL #l_m_half
        RSHIFT #mt 1

        RETURN $returnRegister


LABEL #l_m_double
        LSHIFT #mt 1

        RETURN $returnRegister


EXIT