DEPEND #CONTEXT_MAINPROG


PRIVATE ptr_array_1
PRIVATE ptr_array_2
PRIVATE result


ALLOC 2 &ptr_array_1
COPY &ptr_array_1 &ptr_array_2
NEXT &ptr_array_2

/ ptr_array_2 now points to the second element in the allocated
block. /


SET $ptr_array_1 64.67
SET $ptr_array_2 55.38

        PRINT *ptr_array_1
        PRINT *ptr_array_2


MUL *ptr_array_1 *ptr_array_2 &result


LABEL 1

	CORE_CLEAR

	CORE_DISPLAY


	SUSPEND
	GOTO 1
	
EXIT